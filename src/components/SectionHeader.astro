---
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<div class="w-[448px] max-w-full flex items-center space-x-2 text-primary-700 pt-6">
  <h2 class="text-xl sm:text-2xl md:text-[28px] shrink-0">{title}</h2>
  <div class="w-full h-0 border-t border-primary-700"></div>
</div>
